page{background-color: #EFEFEF;font-size: 30rpx;}
.body{position: relative;width: 100%;}
.queryContainer{width: 694rpx;height: 324px; border-radius:10rpx; margin:  0px auto; background-color:white; position: relative; top: 38rpx;overflow-y: auto;font-size: 30rpx;font-family: PingFangSC-Semibold;}
.queryContainer>view{text-align: center;margin-top: 67px;}
.queryContainer image{width: 150rpx; height: 150rpx;}
.queryContainer text{display: block; color: rgba(22,20,20,0.92); font-weight: bold;font-size: 34rpx;margin-top: 90rpx;}

.footer{width: 694rpx;margin: 0px auto;display: flex;flex-direction: row;margin-top: 180rpx;font-size: 36rpx; letter-spacing: -0.8rpx;}
.footer button{width: 694rpx;height: 106rpx;}

.download{width: 100%; height: 150rpx;position:absolute; bottom: 0px; background-color: rgb(229,229,229);}
.download text{letter-spacing: 0.6rpx;position: relative;line-height: 150rpx; left: 82rpx;font-size: 35rpx;color: rgb(81,79,79);}
.downloadBtn{width: 160rpx;height: 74rpx; font-size: 26rpx;background-color: rgb(55,108,177);color: white;position: absolute;right: 28rpx;top: 35rpx; box-shadow: 0px 8rpx 16rpx 0px rgba(77,120,146,0.38)}

@media screen and (max-width: 320px) 
  	{
      .footer{margin-top: 100rpx;}
      .downloadBtn{font-size: 25rpx;}
    }
