/**index.wxss**/
page{background-color: #EFEFEF;overflow-y: hidden;}

.container{
  width: 100%;
  flex-direction: row;
  padding: 8rpx 0px;
}
.fistBox{
  padding-top: 46rpx;
}
.lastBox{
  margin-top: -22rpx;
}

.body{
  
  position: relative;
}

.container>view{
  display: table;
  text-align: center;
}

.examinationBox{
  margin: 0px auto;
}

.examinationBox image{
  width: 334rpx;
  height: 396rpx;
}

.footer{width: 100%;height: 105rpx;}


.footer image{
  width: 100%;
  height: 108rpx;
  position: absolute;
  bottom: 0px;
}

.examgroup{
  position: relative;
  bottom: 50rpx;
  box-shadow: 0px 12rem 46rpx 0px rgba(0,0,0,0.09);
}

.examgroup image{
  width: 100%;
  height: 110rpx;
  border-radius: 0px -1rpx 0px 0px;
  box-shadow: 0px 4rpx 12rpx 0px rgba(169,174,191,0.42);
  position:absolute;
  left: 0px;
  z-index: 10;
}

.examgroup>text{
  position: relative;
  z-index: 11;
  line-height: 110rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
}