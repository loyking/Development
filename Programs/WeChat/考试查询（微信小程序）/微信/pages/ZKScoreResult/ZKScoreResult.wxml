<view class='body'>
  <view class='queryContainer'>
    <view class="title">查询结果</view>
    <view class='ResultBox'>
      <view>
        <view class='ResultTitle'>科目名称</view>
        <view class='ResultTitle'>科目成绩</view>
      </view>
      <view>
        <view>语文</view>
        <view>{{ChineseScore}}</view>
      </view>
      <view>
        <view>数学</view>
        <view>{{MathematicsScore}}</view>
      </view>
       <view>
        <view>英语</view>
