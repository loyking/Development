      </view>
       <view>
        <view>地理</view>
        <view>{{GeographyScore}}</view>
      </view>
      <view>
        <view>生物</view>
        <view>{{BiologyScore}}</view>
      </view>
      <view>
        <view>化学</view>
        <view>{{ChemistryScore}}</view>
      </view>
      <view>
        <view>物理</view>
        <view>{{PhysicsScore}}</view>
      </view>
      <view>
